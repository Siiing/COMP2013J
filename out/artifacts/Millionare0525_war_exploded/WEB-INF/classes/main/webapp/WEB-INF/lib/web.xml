<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         id="WebApp_ID"
         version="4.0">

  <display-name>Millionare</display-name>
     <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>main.java.com.example.mywebapp.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>main.java.com.example.mywebapp.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>main.java.com.example.mywebapp.SearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>SearchServletOfMoney</servlet-name>
        <servlet-class>main.java.com.example.mywebapp.SearchServletOfMoney</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchServletOfMoney</servlet-name>
        <url-pattern>/search_result_of_money</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SearchServletOfRank</servlet-name>
        <servlet-class>main.java.com.example.mywebapp.SearchServletOfRank</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchServletOfRank</servlet-name>
        <url-pattern>/search_result_of_rank</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SearchServletOfPerson</servlet-name>
        <servlet-class>main.java.com.example.mywebapp.SearchServletOfPerson</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchServletOfPerson</servlet-name>
        <url-pattern>/search_result_of_person</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SearchServletOfSource</servlet-name>
        <servlet-class>main.java.com.example.mywebapp.SearchServletOfSource</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchServletOfSource</servlet-name>
        <url-pattern>/search_result_of_source</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SearchServletOfCountry</servlet-name>
        <servlet-class>main.java.com.example.mywebapp.SearchServletOfCountry</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchServletOfCountry</servlet-name>
        <url-pattern>/search_result_of_country</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SearchServletOfResult</servlet-name>
        <servlet-class>main.java.com.example.mywebapp.SearchServletOfResult</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchServletOfResult</servlet-name>
        <url-pattern>/search_result_of_result</url-pattern>
    </servlet-mapping>


    <!-- Welcome file list -->
    <welcome-file-list>
    	<welcome-file>login.html</welcome-file>
    	<welcome-file>login.jsp</welcome-file>
    	<welcome-file>login.htm</welcome-file>

    	<welcome-file>default.html</welcome-file>
    	<welcome-file>default.jsp</welcome-file>
    	<welcome-file>default.htm</welcome-file>
        
    </welcome-file-list>
  
</web-app>